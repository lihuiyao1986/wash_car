#import <Foundation/Foundation.h>

@interface UIImage(scale)

#pragma 将图片缩放到对应的尺寸
-(UIImage*) scaleToSize:(CGSize)size;

#pragma 等比例压缩图片
-(UIImage *) compressForSize:(CGSize)size;

#pragma 将图片压缩到指定的宽度
-(UIImage *) compressForWidth:(CGFloat)defineWidth;
@end
